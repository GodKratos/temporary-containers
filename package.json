{
  "name": "tmpcontainers",
  "version": "0.0.0",
  "description": "Temporary Containers Plus",
  "scripts": {
    "clean": "rimraf web-ext-artifacts .web-ext-artifacts dist",
    "build": "run-s clean build:webpack",
    "build:webpack": "webpack",
    "beta": "run-s build beta:*",
    "beta:manifest-update-url": "ts-node manifest-update-url.ts",
    "beta:sign": "web-ext sign --channel unlisted -s dist",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint *.js src/**/*.'{ts,js,vue}' test/**/*.ts",
    "lint:tsc": "tsc -p tsconfig.lint.json",
    "webext:build": "web-ext build -s dist",
    "webext:lint": "web-ext lint -s dist",
    "format": "prettier --write '{*,src/**/*,test/**/*}.{html,css,json,yml,md}'",
    "test": "nyc --reporter=html --reporter=text ts-mocha --paths -p tsconfig.json test/*.test.ts",
    "test:functional": "ts-mocha ./test/functional/*.test.ts --timeout 10000",
    "dev": "webpack-dev-server --mode development",
    "watch:test": "ts-mocha --paths -p tsconfig.json test/*.test.ts --watch",
    "watch:test:verbose": "ts-mocha --paths -p tsconfig.json test/*.test.ts --tmp-debug --watch",
    "check:dependencies": "ncu",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GodKratos/temporary-containers.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GodKratos/temporary-containers/issues"
  },
  "homepage": "https://github.com/GodKratos/temporary-containers#readme",
  "dependencies": {
    "delay": "^6.0.0",
    "fomantic-ui": "^2.9.4",
    "jquery": "^3.7.1",
    "jquery-address": "1.6.0",
    "p-queue": "6.4.0",
    "psl": "1.8.0",
    "sortablejs": "1.10.2",
    "vue": "^2.7.16",
    "vuedraggable": "2.23.2"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/chai": "^4.3.20",
    "@types/firefox-webext-browser": "^120.0.4",
    "@types/jquery": "^3.5.32",
    "@types/jquery.address": "^1.5.35",
    "@types/jsdom": "^21.1.7",
    "@types/mocha": "^10.0.10",
    "@types/psl": "^1.1.3",
    "@types/semantic-ui": "^2.2.9",
    "@types/sinon": "^9.0.11",
    "@types/sinon-chai": "^3.2.12",
    "@types/sinon-chrome": "^2.2.16",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "@vue/component-compiler-utils": "^3.3.0",
    "babel-eslint": "^10.1.0",
    "chai": "^5.2.0",
    "chai-deep-match": "^1.2.1",
    "copy-webpack-plugin": "^13.0.0",
    "copyfiles": "^2.4.1",
    "coveralls": "^3.1.1",
    "css-loader": "^3.6.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-vue": "^7.20.0",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^4.3.8",
    "jsdom": "^26.1.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.5.4",
    "mocha": "^11.2.2",
    "npm-check-updates": "^18.0.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^17.1.0",
    "parcel": "^2.14.4",
    "prettier": "2.0.5",
    "rimraf": "^6.0.1",
    "sinon": "^9.2.4",
    "sinon-chai": "^4.0.0",
    "ts-loader": "^9.3.1",
    "ts-mocha": "^11.1.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.8.3",
    "vue-hot-reload-api": "^2.3.4",
    "vue-loader": "^15.11.1",
    "vue-template-compiler": "^2.7.16",
    "vue-typed-mixins": "^0.2.0",
    "web-ext": "^8.6.0",
    "webextensions-api-fake": "^1.3.0",
    "webextensions-geckodriver": "^0.7.0",
    "webpack": "^5.99.8",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1"
  },
  "browserslist": [
    "Firefox >= 67"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{ts,js,vue}": [
      "eslint --fix"
    ],
    "*.{html,css,json,yml,md}": [
      "prettier --write"
    ]
  },
  "private": true,
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "exclude": [
      "test/**/*.ts"
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "singleQuote": true
  },
  "mocha": {
    "bail": true,
    "reporter": "progress",
    "extension": "ts"
  },
  "overrides": {
    "webextensions-api-fake": {
      "sinon": "$sinon"
    }
  }
}
